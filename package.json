{
  "name": "ipfs-ios-backup",
  "description": "Backup iOS devices to IPFS",
  "version": "0.0.0",
  "author": "Cody Hatfield @codynhat",
  "bin": {
    "ipfs-ios-backup": "./bin/run"
  },
  "bugs": "https://github.com/codynhat/ipfs-ios-backup/issues",
  "dependencies": {
    "@oclif/command": "^1.5.19",
    "@oclif/config": "^1.14.0",
    "@oclif/plugin-help": "^2.2.3",
    "@pinata/sdk": "^1.1.3",
    "ipfs-http-client": "^42.0.0",
    "tar": "^6.0.1",
    "tslib": "^1.10.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.22.2",
    "@types/node": "^10.17.15",
    "eslint": "^5.16.0",
    "eslint-config-oclif": "^3.1.0",
    "eslint-config-oclif-typescript": "^0.1.0",
    "globby": "^10.0.2",
    "ts-node": "^8.6.2",
    "typescript": "^3.7.5"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/codynhat/ipfs-ios-backup",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "ipfs-ios-backup",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "codynhat/ipfs-ios-backup",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "echo NO TESTS",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
